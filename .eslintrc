{
  "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
  ],
  "plugins": [
      "react",
      "react-hooks"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "env": {
      "es6": true,
      "browser": true,
      "node": true
  },
  "rules": {
      "react/jsx-filename-extension": [
          1,
          {
              "extensions": [
                  ".js",
                  ".jsx"
              ]
          }
      ],
      "no-func-assign": 0,
      "func-names": 0,
      "comma-dangle": 0,
      "quotes": 0,
      "no-undef": 1,
      "global-require": 0,
      "global-strict": 0,
      "no-extra-semi": 1,
      "no-underscore-dangle": 0,
      "no-console": 0,
      "no-unused-vars": "off",
      "no-unsafe-finally": "off",
      "no-constant-condition": "off",
      "no-trailing-spaces": [
          1,
          {
              "skipBlankLines": true
          }
      ],
      "no-var": 0,
      "newline-after-var": 0,
      "eol-last": 0,
      "no-unreachable": 1,
      "no-alert": 0,
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error",
      "indent": 0,
      "keyword-spacing": 2,
      "import/no-unresolved": 0,
      "no-case-declarations": 1,
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
  }
}